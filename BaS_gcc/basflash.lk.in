MEMORY
{
   flasher (WX) : ORIGIN = TARGET_ADDRESS, LENGTH = 0x00100000		/* target to load basflash */
}

SECTIONS
{
	.text :
	{
		objs/basflash.o(.text)

		*(.data)
		*(.bss)	
		*(.rodata)
		*(.rodata.*)		
	} > flasher
	
	/* Memory mapped registers */
 	__MBAR                  = 0xFF000000;
	___BOOT_FLASH           = 0xe0000000;
 	___BOOT_FLASH_SIZE      = 0x00800000;	
}
